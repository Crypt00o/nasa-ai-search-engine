{
  "name": "nasa-api",
  "version": "1.0.0",
  "description": "Nasa Api Search Engine ",
  "main": "index.js",
  "scripts": {
    "test": "npx tsc && jasmine",
    "dev": "nodemon ./src/app.ts",
    "clean": "npx rimraf build/",
