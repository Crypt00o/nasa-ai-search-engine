    "start": "npm run build &&  node ./build/app.js",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,jsx}'",
    "lint": "eslint . --ext .ts"
  },
  "keywords": [
    "search engine",
    "NASA",
    "NASA API"
  ],
  "author": "Eslam Mohamed Elabd",
  "origin": "Hackanoide @ Nasa Space Apps Hackathon",
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.1224.0",
    "axios": "^0.27.2",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "helmet": "^6.0.0",
    "jasmine": "^4.4.0",
    "jasmine-spec-reporter": "^7.0.0",
    "morgan": "^1.10.0",
    "supertest": "^6.2.4"
  },
  "devDependencies": {
    "@types/aws-sdk": "^2.7.0",
    "@types/axios": "^0.14.0",
    "@types/express": "^4.17.14",
    "@types/jasmine": "^4.3.0",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.7.18",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3",
    "yarn": "^1.22.19"
  }
